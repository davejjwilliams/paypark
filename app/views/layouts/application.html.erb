<!DOCTYPE html>
<html>
  <head>
    <title>Paypark</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= include_gon %>

    <%= javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key=' + Rails.application.credentials.google[:google_maps_api_key]+'&libraries=places'%>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <%= stylesheet_link_tag 'dhtmlxscheduler_material',media:'all','data-turbolinks-track'=>true %>
    <%= javascript_include_tag 'dhtmlxscheduler', 'data-turbolinks-track' => true %>

    <script src="https://js.stripe.com/v3/"></script>

    <%= yield(:head_tags) %>
  </head>

  <body>

  <%= render "shared/header" %>

  <%= yield %>

  <%= render "shared/footer" %>

  </body>
</html>